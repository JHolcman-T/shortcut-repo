{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    // "allowJs": true,
    "lib": ["ESNext", "DOM", "ES2022"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
